{
  "name": "prodig",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm-run-all --parallel build:js build:css copy-assets",
    "build:js": "rimraf dist/*.js && NODE_ENV=production ./node_modules/.bin/rollup -c",
    "build:css": "node 'scripts/sass-build.js'",
    "build-css": "node-sass dist/styles/index.scss -o dist/styles/",
    "clean": "rimraf dist",
    "copy-misc": "node 'scripts/copy-misc.js'",
    "copy-assets": "node 'scripts/copy-assets.js'",
    "format:css": "stylelint './**/*.scss' --fix",
    "format:ts": "tslint './src/**/*.{ts,tsx}' --format verbose",
    "lint:css": "stylelint './**/*.scss'",
    "lint:ts": "tslint './src/**/*.{ts,tsx}'",
    "start": "start-storybook -p 9001 -c .storybook"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/classnames": "^2.2.6",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "autoprefixer": "^9.1.1",
    "awesome-typescript-loader": "^4.0.0",
    "babel-core": "^6.26.3",
    "copyfiles": "^2.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.6",
    "postcss-csso": "^3.0.0",
    "postcss-load-config": "^2.0.0",
    "postcss-modules": "^1.4.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-rebase": "^2.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "^6.0.7",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
